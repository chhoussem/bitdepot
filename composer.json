{
    "name": "dizda/bitdepot",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "require": {
        "php": ">=5.3.9",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "^2.5.2",
        "doctrine/doctrine-bundle": "~1.5",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "guzzlehttp/guzzle": "~5.0",
        "jms/serializer-bundle": "~0.13",
        "oldsound/rabbitmq-bundle": "~1.5",
        "friendsofsymfony/rest-bundle": "~1.4",
        "escapestudios/wsse-authentication-bundle": "~2.0",
        "lexik/jwt-authentication-bundle": "~1.0",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "jms/serializer": "dev-master",
        "pagerfanta/pagerfanta": "~1.0",
        "catchamonkey/console-logger-bundle": "^1.0",
        "doctrine/doctrine-cache-bundle": "^1.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "~2.7",
        "phpunit/phpunit": "5.0.*",
        "satooshi/php-coveralls": "~1.0",
        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "liip/functional-test-bundle": "dev-master",
        "phing/phing": "~2.9"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Nadrieril/serializer"
        }
    ],
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Dizda\\Bundle\\AppBundle\\Composer\\ScriptHandler::installConfigFiles",
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.8-dev"
        }
    }
}
